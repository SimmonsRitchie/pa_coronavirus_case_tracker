    "d3-scale": "^3.2.1",
    "moment": "^2.24.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "raf": "3.3.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-simple-maps": "^1.0.0-beta.0",
    "react-spinners": "^0.6.1",
